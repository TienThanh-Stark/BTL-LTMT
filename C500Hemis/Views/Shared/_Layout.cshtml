<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>@ViewData["Title"] - C500Hemis</title>
    <link href="https://cdn.jsdelivr.net/npm/simple-datatables@7.1.2/dist/style.min.css" rel="stylesheet" />
    <script src="https://use.fontawesome.com/releases/v6.3.0/js/all.js" crossorigin="anonymous"></script>
    <link href="~/css/styles.css" rel="stylesheet" />
</head>
<body class="sb-nav-fixed">
    <header>
        <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
            <!-- Navbar Brand-->
            <a class="navbar-brand ps-3" asp-area="" asp-controller="Home" asp-action="Index"><img src="~/assets/img/logo_hoc_vien_annd.png" style="margin-right: 5px; height: 26px;" />Cơ Sở Giáo Dục</a>
            <!-- Sidebar Toggle-->
            <button class="btn btn-link btn-sm order-1 order-lg-0 me-4 me-lg-0" id="sidebarToggle" href="#!"><i class="fas fa-bars"></i></button>
            <p class="d-none d-md-inline-block form-inline ms-auto me-0 me-md-3 my-2 my-md-0" style="color: white">
                Nhóm 6 B9D55
            </p>
            <!-- Navbar-->
            <ul class="navbar-nav ms-auto ms-md-0 me-3 me-lg-4">
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" id="navbarDropdown" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="fas fa-user fa-fw"></i></a>
                   
                </li>
            </ul>
        </nav>

        <div id="layoutSidenav">
            <div id="layoutSidenav_nav">
                <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                    <div class="sb-sidenav-menu">
                        <div class="nav">
                            <div class="sb-sidenav-menu-heading">THÔNG TIN</div>
                            <a class="nav-link" href="/CoCauToChuc">
                                <div class="sb-nav-link-icon"><i class="fas fa-tachometer-al"></i></div>
                                Cơ Cấu Tổ Chức
                            </a>
                            <a class="nav-link" href="/CoSoGiaoDuc">
                                <div class="sb-nav-link-icon"><i class="fas fa-chart-area"></i></div>
                                Cơ Sở Giáo Dục
                            </a>
                            <a class="nav-link" href="/DanhHieuThiDuaGiaiThuongKhenThuongCuaCoSoGd">
                                <div class="sb-nav-link-icon"><i class="fas fa-table"></i></div>
                                Danh Hiệu Thi đua Giải Thưởng, Khen Thưởng Của Cơ Sở Giáo Dục
                            </a>
                            <a class="nav-link" href="/DauMoiLienHe">
                                <div class="sb-nav-link-icon"><i class="fas fa-columns"></i></div>
                                Đầu Mối Liên Hệ
                            </a>
                            <a class="nav-link" href="/DonViLienKetDaoTaoGiaoDuc">
                                <div class="sb-nav-link-icon"><i class="fas fa-book-open"></i></div>
                                Đơn Vị Liên Kết Đào Tạo Giáo Dục
                            </a>
                            <a class="nav-link" href="/KhoaHoc">
                                <div class="sb-nav-link-icon"><i class="fas fa-chart-area"></i></div>
                                Khóa Học
                            </a>
                            <a class="nav-link" href="/LichSuDoiTenTruong">
                                <div class="sb-nav-link-icon"><i class="fas fa-tachometer-al"></i></div>
                                Lịch Sử Đổi Tên Trường
                            </a>
                            <a class="nav-link" href="/ToChucKiemDinh">
                                <div class="sb-nav-link-icon"><i class="fas fa-chart-area"></i></div>
                                Tổ chức Kiểm Định
                            </a>
                            <a class="nav-link" href="/VanBanTuChu">
                                <div class="sb-nav-link-icon"><i class="fas fa-table"></i></div>
                                Văn Bản Từ Chữ
                            </a>
                        </div>
                    </div>
                   
                </nav>
            </div>
            <div id="layoutSidenav_content">
                <div class="container-fluid px-4">
                    <main role="main">
                        @RenderBody()
                    </main>
                </div>
                <footer class="py-4 bg-light mt-auto">
                    <div class="container-fluid px-4">
                        <div class="d-flex align-items-center justify-content-between small">
                            <div class="text-muted">Copyright &copy; Nhóm 6</div>
                            
                        </div>
                    </div>
                </footer>
            </div>
        </div>
        <script src="https://cdn.jsdelivr.net/npm/simple-datatables@7.1.2/dist/umd/simple-datatables.min.js" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
        <script src="~/js/scripts.js"></script>
    </header>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
